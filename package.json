{
  "name": "svrx-plugin-webpack",
  "version": "0.0.3",
  "description": "webpack plugin for svrx",
  "keywords": [
    "svrx",
    "svrx-plugin"
  ],
  "engines": {
    "svrx": "^0.0.4"
  },
  "homepage": "https://svrx.gitbook.io/docs/",
  "license": "MIT",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha __tests__/",
    "lint": "eslint \"**/*.js\"",
    "fix": "eslint \"**/*.js\" --fix",
    "clean": "rimraf -rf node_modules"
  },
  "dependencies": {
    "koa-compose": "^4.1.0",
    "resolve": "^1.11.0",
    "semver": "^6.1.2",
    "svrx-util": "^0.0.3",
    "webpack": "^4.0.0",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "coveralls": "^3.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "expect.js": "^0.3.1",
    "husky": "^1.3.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.0",
    "rimraf": "^2.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/x-orpheus/svrx-plugin-webpack.git"
  },
  "author": "x-orpheus",
  "bugs": {
    "url": "https://github.com/x-orpheus/svrx-plugin-webpack/issues"
  }
}
